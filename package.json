    "@pigment-css/react": "^0.0.30",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.64.2",
    "@types/ioredis": "^4.28.10",
    "@types/pg": "^8.15.2",
    "@vercel/kv": "^1.0.1",
    "@xmtp/browser-sdk": "^2.1.1",
    "@xmtp/node-sdk": "^2.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.5.0",
    "eruda": "^3.2.3",
    "ethers": "^6.11.1",
    "ioredis": "^5.6.1",
    "jiti": "^2.0.0",
